<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <body>
    <% if(Array.isArray(users) && users.length > 0) { %>
    <table>
      <thead>
        <tr>
          <th>Sno</th>
          <th>name</th>
          <th>email</th>
          <th>password</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach((element, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= element.name %></td>
          <td><%= element.email %></td>
          <td><%= element.password %></td>
          <td><%= element.role %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <% } else { %>
    <p>users Not found</p>
    <% } %>
  </body>
</html>
